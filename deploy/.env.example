# Deploy configuration
# Copy this to .env and fill in your values

# Required: SSH private key path
DEPLOY_SSH_KEY=~/.ssh/id_rsa

# Required: SSH target (user@host)
DEPLOY_SERVER=root@your-server.com

# Optional: Remote directories (defaults shown)
# DEPLOY_REMOTE_DIR=/opt/piportal
# DEPLOY_DOWNLOADS_DIR=/var/www/piportal/downloads
